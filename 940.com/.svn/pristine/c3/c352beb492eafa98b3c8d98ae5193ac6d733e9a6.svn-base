#kd_content{width: 100%;min-width: 1200px;background: #F6F6F6 url('../images/reg_bg.png') repeat-x;overflow: hidden;margin-top: -1px;}
.wrapLink a .iconp,.wrapLink a b{background: url('../images/shop_icon1.png') no-repeat;}
#wrapShop{width: 1200px;margin: 30px auto;padding:0px;background: #FFFFFF;padding-bottom: 30px;}
h2.title_txt{height: 35px;line-height: 35px;font-size: 20px;font-weight: 500;border-bottom: 1px solid #DDDDDD; width: 1144px;padding-top: 30px;
    margin: 0 auto;margin-bottom: 20px;}
h2.title_txt span {display: inline-block;height: 34px;padding: 0 5px;border-bottom: 2px solid #1BB3BC;color: #FF6601;}
h2.title_txt em{font-size: 15px;color: #1BB3BC;position: relative;top: 2px;}
h2.title_txt a{font-size: 12px;color: #919191;position: relative;top: 5px;*top: -30px;}
h2.title_txt a:hover{text-decoration: underline;}
.page_link{width: 100%;padding-bottom: 30px;margin-bottom: 10px;border-bottom: 1px solid #DEDEDE;}
.page_link .wrapLink{width: 100%;height: 190px;background-color: #4CCCD8;transition: 0.3s;}
.page_link .wrapLink a{float: left;width: 16.666666667%;height: 100%;position: relative;transition: 0.3s;}
.wrapLink a .iconp{display: block;width: 100%;margin-top: 40px;padding-top: 80px;text-align: center;font-size: 20px;color: #fff;}
.wrapLink a b{position: absolute;width: 19px;height: 19px;left: 50%;bottom: -41px;margin-left: -9px;background-color: #fff;}

.wrapLink a.cur.page1,.wrapLink a.page1:hover{background-color: #24BFCE;}
.wrapLink a.page1 .iconp{background-position:41px 3px }
.wrapLink a.page1 b{background-position: -1001px 0;}

.wrapLink a.cur.page2,.wrapLink a.page2:hover{background-color: #26AAE4;}
.wrapLink a.page2 .iconp{background-position:-120px 3px }
.wrapLink a.page2 b{background-position: -1001px 0;}

.wrapLink a.cur.page3,.wrapLink a.page3:hover{background-color: #30C783;}
.wrapLink a.page3 .iconp{background-position:-286px 3px }
.wrapLink a.page3 b{background-position: -1001px 0;}


.wrapLink a.cur.page4,.wrapLink a.page4:hover{background-color: #24BFCE;}
.wrapLink a.page4 .iconp{background-position:-470px 3px }
.wrapLink a.page4 b{background-position: -1001px 0;}

.wrapLink a.cur.page5,.wrapLink a.page5:hover{background-color: #26A8E4;}
.wrapLink a.page5 .iconp{background-position:-637px 3px }
.wrapLink a.page5 b{background-position: -1001px 0;}

.wrapLink a.cur.page6,.wrapLink a.page6:hover{background-color: #8585E9;}
.wrapLink a.page6 .iconp{background-position:-800px 3px }
.wrapLink a.page6 b{background-position: -1001px 0;}

.wrapLink a.cur b,.wrapLink a:hover b{background-position: -1001px -24px;}

.kd_list_ul{width: 1144px;margin: 0 auto;}
.kd_list_ul li{height: 250px;width: 100%;}
.kd_list_ul li .kd_box{width: 380px;height:248px;border:1px solid #DFDFDF;float: left;margin-right: 10px;}
.kd_list_ul li .kd_box .topa{width: 100%;height: 105px;display: block;overflow: hidden;}
.kd_box .topa em{display: block;width: 140px;}
.kd_box .topa span{width: 230px;display: block;color: #fff;padding-left: 10px;padding-top: 20px;}
.kd_box .topa span strong{font-size: 26px;font-weight: normal;}
.kd_box .topa span i{margin-top: 10px;display: inline-block;}
.kd_box .txtp{width: 85%;margin: 10px auto;line-height: 20px;height: 80px;overflow: hidden;}
.kd_box .linka{display: block;width: 130px;height: 30px;text-align: center;line-height: 30px;color: #333333;background-color: #EFEFEF;margin: 0 auto;}
.kd_box .linka:hover{color: #FF6601;}
.kd_list_ul li .kd_dl{width: 350px;height: 100%;}
.kd_list_ul li .kd_dl dt{height: 30px;line-height: 30px;font-size: 20px;margin-top: 10px;padding-left: 20px;position: relative;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}
.kd_list_ul li .kd_dl dt a{color: #FF6601;font-size: 18px;}
.kd_list_ul li .kd_dl dt:before{position: absolute;content: '';display: block;height: 5px;width: 5px;background-color: red;left: 0px;top: 14px;}
.kd_list_ul li .kd_dl dd{font-size: 14px;padding-left: 20px;position: relative;margin-top: 13px;color: #333333;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}
.kd_list_ul li .kd_dl dd:before{position: absolute;content: '';display: block;height: 5px;width: 5px;background-color: #333;left: 0px;top: 8px;}
.kd_list_ul li .kd_dl dd a:hover{color: #FF6601;}

.kd_list_ul li .kd_box .a1{background-color: #EB0111;}
.kd_list_ul li .kd_box .a2{background-color: #2A93CA;}
.kd_list_ul li .kd_box .a3{background-color: #F7A900;}
.kd_list_ul li .kd_box .a4{background-color: #363435;}
.kd_list_ul li .kd_box .a5{background-color: #A2D30E;}
.kd_list_ul li .kd_box .a6{background-color: #8971CF;}

/*开店技巧*/
#wrapJiqiao{width: 1144px;margin: 30px auto;padding:0px 28px;background: #FFFFFF;padding-bottom: 30px;overflow: hidden;}
#wrapJiqiao .j_tab_ul{width: 100%;height: 35px;padding-top: 30px;border-bottom: 1px solid #DDDDDD;}
.j_tab_ul li{padding: 0 10px;float: left;height: 34px;line-height: 35px;margin-right: 10px;}
.j_tab_ul li a{font-size: 16px;display: block;}
.j_tab_ul li.cur{border-bottom: 2px solid #1BB3BC;}
.j_tab_ul li.cur a{font-size: 18px;color: #FF6601;}

.j_list_ul{width: 100%;}
.j_list_ul li{width: 50%;height: 125px;border-bottom: 1px solid #DFDFDF;float: left;padding: 15px 0;}
.j_list_ul li p{width: 140px;height: 105px;padding: 10px;border:1px solid #DDDDDD;float: left;overflow: hidden;}
.j_list_ul li p a{display: block;width: 140px;height: 105px;overflow: hidden;}
.j_list_ul li p img{width: 140px;}
.j_list_ul li .wrapTxtDl{width: 400px;height: 120px;float: left;margin-left: 10px;padding: 5px 0;}
.wrapTxtDl dt{}
.wrapTxtDl dt a{font-size: 16px;color: #4D4C4A;display: inline-block; white-space: nowrap;text-overflow: ellipsis;overflow: hidden;max-width: 380px;}
.wrapTxtDl dt a:hover{color: #FF6601;}
.wrapTxtDl dt img{position: relative;top: -3px;}
.wrapTxtDl dd{font-size: 14px;margin-top: 15px;}
.wrapTxtDl dd span{color: #2BA532;}

 .tcdPageCode{padding-top: 20px;text-align: center;*padding-top: 42px;}
 .tcdPageCode a{padding: 5px 10px;border:1px solid #DDDDDD;color: #7F7F7F;border-radius: 2px;font-size: 12px;margin-left: 5px;display: inline-block;
	background-image: linear-gradient(top, #FEFEFE, #F2F2F2);
	background-image: -moz-linear-gradient(top, #FEFEFE, #F2F2F2);
	background-image: -webkit-linear-gradient(top, #FEFEFE, #F2F2F2);
	background-image: -ms-linear-gradient(top, #FEFEFE, #F2F2F2);
	background-image: -o-linear-gradient(top, #FEFEFE, #F2F2F2);
}
.tcdPageCode a:hover{color: #FFBA00;}
 .tcdPageCode a.nextPage{padding: 5px 20px 5px 5px;position: relative;}
 .tcdPageCode a.nextPage b{position: absolute;display: inline-block;width: 0;height: 0;border-width: 6px;border-style: dashed;border-color: transparent;border-left-color: #7F7F7F;border-left-style: solid;top: 7px;right: 1px;}
 .tcdPageCode a.prevPage{padding: 5px 5px 5px 20px;position: relative;}
 .tcdPageCode a.prevPage b{position: absolute;display: inline-block;width: 0;height: 0;border-width: 6px;border-style: dashed;border-color: transparent;border-right-color: #7F7F7F;border-right-style: solid;top: 8px;left: 1px;}
 .tcdPageCode span{margin-left: 5px;}
 .tcdPageCode span.current{background: #FFBA00;border:0;padding: 6px 11px;color: #fff;border-radius: 2px;display: inline-block;}


/* 开店技巧*/
#wrapJiqiao .j_details{width: 100%;margin-top: 20px;}
.j_details .l_hot{width: 280px;}

.j_details .l_hot .k_hot h1{height: 50px;width: 100%;background: #61CEF2;text-align: center;line-height: 50px;font-size: 24px;color: #fff;font-weight: 500;}

.k_hot{float: left;width: 280px;border-top: 0;overflow: hidden;}
.k_hot .top5box{width: 100%;vertical-align: middle;border: #F0F0F0 1px solid;border-top:none;box-sizing: border-box;}
.k_hot .top5box li {width: 100%;padding: 12px 0 0;height: 30px;border-top: #F0F0F0 1px solid;vertical-align: middle;/*transition: all .5s;*/
}
.k_hot .top5box li .pic {float: left;width: 120px;height: 90px;padding: 0 8px;display: none;}
.k_hot .top5box li .pic img {width: 120px;vertical-align: top;background-size: 50%;}
.k_hot .top5box li .title {float: left;}
.k_hot .top5box li .title a {float: left;width: 170px;line-height: 20px\9;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.k_hot .top5box li span{display: none;}
.k_hot .top5box li .title i {float: left;width: 18px;height: 18px;margin: 0px 5px 0 8px;display: inline;font: 12px/20px Arial;
text-align: center;color: #333;border:1px solid #ddd;}
.k_hot .top5box .hover {height: 100px;padding-top: 7px;background: #fff;}
.k_hot .top5box .hover .title a {width: 98%;height: 35px;white-space: normal;line-height: 18px;}
.k_hot .top5box .hover .title a:hover{color: #FF6601;}
.k_hot .top5box .hover .title i {float: left;position: relative;margin: -5px 0 0 -128px;background: #FF7302;color: #fff;border:none;width: 20px;height: 20px;}
.k_hot .top5box .hover .title {width: 140px;margin-top: 5px;}
.k_hot .top5box .hover span{float: left;margin-top: 10px;display: block;width:50%;font-size: 14px;color: #666;}

.k_hot .top5box .hover .pic{display: block;}
.k_hot .top5box .hover del {width: 102px;color: #999;display:inline;
}

